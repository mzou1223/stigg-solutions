{
  "name": "@stigg/node-server-sdk",
  "version": "3.86.0",
  "description": "Stigg server-side node SDK",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "repository": "https://github.com/stiggio/node-server-sdk",
  "license": "MIT",
  "keywords": [],
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "build:watch": "tsc -p tsconfig.build.json --watch",
    "fix": "run-s fix:*",
    "fix:prettier": "prettier \"{src,test}/**/*.ts\" --write",
    "fix:lint": "eslint src --ext .ts --fix",
    "test": "jest --forceExit",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "reset-hard": "git clean -dfx && git reset --hard && yarn",
    "prepare-release": "run-s reset-hard test doc:html",
    "prepare": "husky install",
    "run": "ts-node examples/runSdk.ts",
    "pretty-quick": "pretty-quick --staged --pattern '**/*.*(js|jsx|ts|tsx)'",
    "lint": "eslint \"{src,examples,test}/**/*.ts\" --fix",
    "docs": "typedoc"
  },
  "files": [
    "dist/**/*"
  ],
  "engines": {
    "node": ">=10"
  },
  "dependencies": {
    "@apollo/client": "3.4.16",
    "@bitauth/libauth": "^1.17.1",
    "@graphql-codegen/fragment-matcher": "^3.2.1",
    "@stigg/api-client-js": "3.59.2",
    "@types/ws": "^8.2.3",
    "axios": "^1.8.2",
    "axios-retry": "^4.0.0",
    "cross-fetch": "^3.1.5",
    "dayjs": "^1.11.13",
    "graphql": "^15.6.1",
    "graphql-ws": "^5.6.0",
    "husky": "^7.0.2",
    "ioredis": "^5.2.4",
    "isomorphic-ws": "^4.0.1",
    "lodash": "^4.17.21",
    "lru-cache": "^10.0.1",
    "nanoid": "^3.3.8",
    "redlock": "^5.0.0-beta.2",
    "tiny-typed-emitter": "^2.1.0",
    "ws": "^8.17.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^13.2.1",
    "@commitlint/config-conventional": "^13.2.0",
    "@types/glob": "^8.0.1",
    "@types/jest": "^27.0.2",
    "@types/lodash": "^4.14.175",
    "@types/node": "^16.11.1",
    "@types/tmp": "^0.2.3",
    "@typescript-eslint/eslint-plugin": "^4.0.1",
    "@typescript-eslint/parser": "^4.0.1",
    "eslint": "^7.8.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-functional": "^3.0.2",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-prettier": "^4.0.0",
    "glob": "^8.1.0",
    "jest": "^27.2.5",
    "lint-staged": "^11.2.3",
    "msw": "^0.35.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.1.1",
    "pretty-quick": "^3.1.3",
    "redis-memory-server": "^0.12.1",
    "standard-version": "^9.0.0",
    "ts-jest": "^27.0.7",
    "ts-node": "^9.0.0",
    "typedoc": "^0.22.13",
    "typedoc-plugin-rename-defaults": "^0.4.0",
    "typescript": "^4.0.2"
  },
  "publishConfig": {
    "access": "public"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "prettier --check",
      "eslint --cache --fix"
    ]
  }
}
